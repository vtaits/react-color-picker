{
  "name": "@vtaits/react-color-picker",
  "version": "0.1.3",
  "description": "React Color Picker",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "/dist",
    "/es",
    "/lib"
  ],
  "scripts": {
    "start": "cross-env BABEL_ENV=dev webpack-dev-server",
    "build": "yarn build:cjs && yarn build:es && yarn build:css",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir lib --ignore \"src/**/__tests__\"",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es --ignore \"src/**/__tests__\"",
    "build:css": "postcss style/**/*.css --dir dist",
    "clean": "rm -rf lib es dist",
    "lib": "yarn clean && yarn babel",
    "babel": "babel --out-dir lib src",
    "prepare": "yarn clean && yarn build",
    "lint": "BABEL_ENV=es eslint src --ext .js,.jsx",
    "test": "yarn lint"
  },
  "keywords": [
    "react",
    "react-component",
    "color",
    "picker",
    "color-picker"
  ],
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "classnames": "^2.3.1",
    "drag-helper": "^1.3.6",
    "prop-types": "^15.8.1",
    "region": "^2.1.2",
    "tinycolor2": "^1.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "autoprefixer": "^10.4.8",
    "babel-loader": "^8.2.5",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "postcss": "^8.4.16",
    "postcss-cli": "^10.0.0",
    "precss": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.0"
  },
  "peerDependencies": {
    "react": "^16.3.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.3.0 || ^17.0.0 || ^18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vtaits/react-color-picker.git"
  },
  "bugs": {
    "url": "https://github.com/vtaits/react-color-picker/issues"
  },
  "license": "MIT",
  "author": "Vadim Taits <taizvadim@gmail.com>"
}
